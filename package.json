{
  "name": "form-build",
  "version": "3.0.0",
  "private": false,
  "main": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --rules-dir packages",
    "fix": "eslint --fix ./packages/**/* ./src/**/*",
    "docs:dev": "nodemon --ext md,vue --watch .vuepress --watch . --exec vuepress dev docs-src --no-cache",
    "docs:dev:nocache": "vuepress dev docs-src --no-cache",
    "docs:build": "vuepress build docs-src",
    "lib": "vue-cli-service build --target lib --name form-build --report --dest lib ./packages/index.ts",
    "pub:doc": "sh build/build_doc.sh"
  },
  "dependencies": {
    "dayjs": "^1.8.28",
    "less-loader": "^6.1.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.1",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-service": "~4.3.0",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vuepress/plugin-active-header-links": "^1.5.2",
    "@vuepress/plugin-register-components": "^1.5.2",
    "async-validator": "^3.3.0",
    "codemirror": "^5.55.0",
    "csstype": "^2.6.10",
    "element-ui": "^2.13.2",
    "eslint": "^7.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "nodemon": "^2.0.4",
    "typescript": "~3.8.3",
    "vant": "^2.8.4",
    "vuepress": "^1.5.2",
    "vuepress-plugin-clean-urls": "^1.1.1"
  }
}
